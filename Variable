public class Variable {
	
	public static void main(String[] args) {
		
		System.out.println("Hello");
		
		/*
		  this
		  is
		  commend		  
		 */
		
		//declare a variable (int - Number)
		
		int age = 21;
		System.out.println(age);
		
		//Double - float 
		
		double price = 300.32;
		System.out.println(price);
		
		//char - character
		
		char grade = 'A';
		char symbol = '!';
		char currency = '$';
		
		System.out.println(grade);
		
		//boolean - True or False
		
		boolean myProject = true;
		boolean myOutput = false;
		
		if(myOutput) {
			System.out.println("My Output is Running");
		}
		
		else {
			System.out.println("My Output is not Running");
		}
		
		//String 
		
		String name = "Jeslin";
		
		System.out.println("Hello my name is "+name);
		
		
		
	}

}
